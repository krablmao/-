#include <iostream>
using namespace std;

 int main()
 {
 	printf("Друкуємо таблицю квадратів цілих позитивних чісел");
 int k,p;
  for (int i = 0; i <10 ; i++) {
  	int k=i+1; 
  	int p=k*k;
  	if(k%2!=0)
  	printf("\n\tчисло %3d в квадраті дорівнює %3d",k,p);
 }
 cout << "\n\n\t";
 system("pause");
 return 0;
}
